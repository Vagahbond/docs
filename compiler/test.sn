struct Person {
  firstname: &str
  lastname: &str
  age: i32

  fn new() -> Person {
    {
      firstname: "San",
      lastname: "Chan",
    }
  }

  fn test(&self, variable: bool): f64 {
    return 0x78 + (432 - 12.43) * variable / age
  }
}

fn main(): i32 {
  let mut p: Person = Person::new()
  p.test(true)

  return 0
}
